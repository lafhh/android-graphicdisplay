<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.js.graphicdisplay">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.MenuActivity">
        </activity>
        <activity android:name=".activity.GraphicActivity"
                  android:label="@string/label_funds"
        >
        </activity>
        <activity android:name=".activity.RERGraphicActivity"
                  android:label="@string/label_rer"
        >
        </activity>
        <activity android:name=".activity.ReserveGraphicActivity"
                  android:label="@string/label_reserve"
        >
        </activity>
        <activity android:name=".activity.RMGraphicActivity"
                  android:label="@string/label_rm"
        >
        </activity>
        <activity android:name=".activity.SalesGraphicActivity"
                  android:label="@string/label_sales"
        >
        </activity>
    </application>

</manifest>